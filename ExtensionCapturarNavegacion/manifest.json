{
	"manifest_version": 2,
	"name": "Capturar Evento",
	"version": "1.0",
	"description": "Extension para Capturar los Eventos producidos en el navegador.",
	"permissions": [
		"tabs",
		"activeTab",
		"notifications",
		"downloads"
	],
	"content_scripts": [
    	{
     		"matches": ["<all_urls>"],
     		"js": ["content.js"]
    	}
  	],
  	"background" : {
	   "scripts" : ["background.js"]
	},
	"icons": {
		"16" : "img/icon16.png",
		"48" : "img/icon48.png",
		"128": "img/icon128.png"
		},
	"browser_action" : {
		"default_icon" : "img/desactivado.png"
	}
}